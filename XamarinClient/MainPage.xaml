<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:xamarinclient="clr-namespace:XamarinClient" 
             x:DataType="xamarinclient:MainViewModel"
             x:Class="XamarinClient.MainPage">

    <StackLayout>
        <CheckBox IsChecked="{Binding ReceiverFlag}" />
        <Entry Text="{Binding SocketValue}" />
    </StackLayout>

</ContentPage>
